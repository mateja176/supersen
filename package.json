{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "postinstall": "patch-package",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "echo test",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "lint": "eslint ./**/*.{ts,tsx}",
    "lint:prod": "npm run lint -- --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier **/*.{ts,tsx} -c",
    "format:fix": "npm run format -- --write",
    "ci": "cross-env CI=true concurrently --colors --kill-others-on-fail --names lint,format,check,test --prefix-colors blue,cyan,purple,yellow npm:lint:prod npm:format npm:check npm:test"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "expo": "~43.0.0",
    "expo-linear-gradient": "^10.0.3",
    "expo-status-bar": "~1.1.0",
    "qs": "^6.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "^0.64.3",
    "react-native-root-toast": "^3.3.0",
    "react-native-web": "^0.17.1",
    "react-query": "^3.32.1",
    "tinycolor2": "^1.4.2",
    "zod": "^3.11.6",
    "zustand": "^3.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/qs": "^6.9.7",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.3.0",
    "typescript": "~4.3.5"
  },
  "private": true
}
