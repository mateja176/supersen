{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "echo test",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "lint": "eslint ./**/*.{ts,tsx}",
    "lint:prod": "npm run lint -- --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier **/*.{ts,tsx} -c",
    "format:fix": "npm run format -- --write",
    "ci": "cross-env CI=true concurrently --colors --kill-others-on-fail --names lint,format,check,test --prefix-colors blue,cyan,purple,yellow npm:lint:prod npm:format npm:check npm:test"
  },
  "dependencies": {
    "@emotion/native": "^11.0.0",
    "@emotion/react": "^11.5.0",
    "expo": "~43.0.0",
    "expo-status-bar": "~1.1.0",
    "react": "17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-native": "0.64.2",
    "react-native-web": "^0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-color": "^3.0.6",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.3.0",
    "typescript": "~4.3.5"
  },
  "private": true
}
